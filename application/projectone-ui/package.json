{
  "name": "projectone-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "npm run i18n && ng",
    "start": "npm run i18n && concurrently 'ng serve -o' 'npm run i18nw' ",
    "build": "npm run i18n && ng build",
    "test": "npm run i18n && ng test",
    "testci": "npm run i18n && ng test --watch=false",
    "lint": "ng lint",
    "e2e": "npm run i18n && ng e2e",
    "prestart": "npm install",
    "ghpages": "npm run i18n &&  ng build --prod --env=prod --aot false --base-href /P1UI/  --sourcemap=false && gulp publish",
    "build-local": "npm run i18n && ng build --prod --env=local --aot false --base-href /p1/  ",
    "i18n": "npm run p1I18n --prefix ../../tools/development/i18n",
    "i18nw": "npm run p1I18nw --prefix ../../tools/development/i18n"
  },
  "private": true,
  "dependencies": {
    "@pearson/angular-material": "^0.2.0",
    "@types/crypto-js": "^3.1.33",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "^4.2.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "concurrently": "^3.4.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "ts-node": "^3.0.2",
    "tslint": "~4.5.0",
    "typescript": "~2.3.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-load-plugins": "^1.1.0"
  }
}
